    <section style="background-color: #eee;" class="">
        <div class="container">
            <div class="row justify-content-center">
                <div class="walletDiv">
                    <div class="card">
                        <div class="card-body text-center outerDiv">
                            <h1>Wallet</h1>
                            <h2><%= walletAmount %></h2>
                            <hr>
                            <% if( orders.length> 0 ) { %>
                                <% for( let i=0 ; i < orders.length ; i++){ %>
                                    <% for( let j=0 ; j < orders[i].products.length ; j++){ %>
                                            <% if( orders[i].products[j].refundAmount ){%>
                                            <div class="card " style="width: 18rem;">
                                            <ul class="list-group list-group-flush">
                                            <li class="list-group-item">
                                            <p>For <%=orders[i].products[j].status %>  "<%= orders[i].products[j].productId.productName %>"</p> + <%= orders[i].products[j].refundAmount %>
                                            </li>
                                            </ul>
                                            </div>
                                            <% } %>
                                            <% if( orders[i].products[j].orderedAmountWallet ){%>
                                                <div class="card " style="width: 18rem;">
                                                <ul class="list-group list-group-flush">
                                                <li class="list-group-item">
                                                <p>For ordered  "<%= orders[i].products[j].productId.productName %>"</p> - <%= orders[i].products[j].orderedAmountWallet %>
                                                </li>
                                                </ul>
                                                </div>
                                            <% } %>
                                    <% } %>
                                <% } %>
                            <% } %>
                            <% if( usersReferredFrom.length> 0 ) { %>
                                <% for( let k=0 ; k < usersReferredFrom.length ; k++){ %>
                                    <div class="card " style="width: 18rem;">
                                        <ul class="list-group list-group-flush">
                                        <li class="list-group-item">
                                        <p>For referred  "<%= usersReferredFrom[k].name %>"</p> + 100
                                        </li>
                                        </ul>
                                        </div>
                                <% } %>
                            <% } %>  
                            <% if( referredFromUser ) { %>
                               
                                    <div class="card " style="width: 18rem;">
                                        <ul class="list-group list-group-flush">
                                        <li class="list-group-item">
                                        <p>For registered by referralCode of  "<%= referredFromUser.name %>"</p> + 100
                                        </li>
                                        </ul>
                                        </div>
                                
                            <% } %>  


                           

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
