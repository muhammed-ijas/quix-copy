				<h3 class="tittle-w3layouts my-lg-4 my-4">New Arrivals for you </h3>
				<div class="row">
					<% if(products.length> 0) { %>
						<% for(let i=0; i < products.length; i++) { %>
							<!-- /womens -->
							<div class="col-md-3 product-men women_two">
								<div class="product-googles-info googles">
									<div class="men-pro-item">
										<div class="men-thumb-item">
											<img src="/uploads/<%= products[i].images[0] %>" class="img-fluid" alt="">
											<div class="men-cart-pro">
												<div class="inner-men-cart-pro">
													<a href="/singleProduct?id=<%= products[i]._id %>"
														class="link-product-add-cart">Quick View</a>
												</div>

											</div>
											<div>
												<% if(products[i].stock==0) { %>
													<p style="color: red;">Out of Stock</p>
													<% } %>
											</div>

											<% if(products[i].offerPercentage) { %>

												<span class="product-new-top">
													<%= products[i].offerPercentage %>%
												</span>

												<% } %>

										</div>
										<button type="submit" class="addToCartBtn"
											data-product-id="<%= products[i]._id %>">
											<i class="fa fa-shopping-cart"></i>
										</button>
										<div class="item-info-product">
											<div class="info-product-price">
												<div class="grid_meta">
													<div class="product_price">
														<h4>
															<a href="/singleProduct?id=<%= products[i]._id %>">
																<%= products[i].productName %>
															</a>
														</h4>
														<div class="grid-price mt-2">
															<% if(products[i].offerPercentage) { %>

																<p
																	style="font-size: smaller; text-decoration: line-through; color: red;">
																	<%= products[i].prize %>
																</p>

																<span class="money ">&#8377;<%= products[i].offerPrice
																		%></span>



																<% }else{ %>
																	<span class="money ">&#8377;<%= products[i].prize %>
																			</span>
																	<% } %>
														</div>
													</div>
													<ul class="stars">
														<% if(products[i].reviews.length> 0) { %>
															<% let totalRating=0; %>
																<% products[i].reviews.forEach(review=> { %>
																	<% totalRating +=review.rating; %>
																		<% }); %>
																			<% let averageRating=totalRating /
																				products[i].reviews.length; %>
																				<% let
																					roundedRating=Math.round(averageRating
																					* 10) / 10; %>
																					<!-- Display stars based on roundedRating -->
																					<% for (let j=0; j < 5; j++) { %>
																						<% if (j <
																							Math.round(roundedRating)) {
																							%>
																							<i
																								class="fa fa-star  starss"></i>
																							<% } else { %>
																								<i
																									class="fa fa-star-o starss"></i>
																								<% } %>
																									<% } %>

																										<% } else { %>
																											<p>No
																												reviews
																												yet</p>
																											<% } %>
													</ul>
												</div>
												<div class="googles single-item hvr-outline-out">
													<form action="" method="">



													</form>

												</div>
											</div>
											<div class="clearfix"></div>
										</div>
									</div>
								</div>
							</div>
							<% } %>
								<% } %>







									<!-- /mens -->
