		.images11 {

			background-size: 400% 400%;
			animation: gradient 15s ease infinite;

			background-color: black;
			font-family: "Muli", sans-serif;
			font-size: 69px;
			display: flex;
			align-items: center;
			justify-content: center;
			height: 100vh;

			margin: 0;
		}

		.container {
			display: flex;
			width: 90vw;
		}

		@keyframes gradient {
			0% {
				background-position: 0% 50%;
			}

			50% {
				background-position: 100% 50%;
			}

			100% {
				background-position: 0% 50%;
			}
		}

		.panel {
			background-size: auto 100%;
			background-position: center;
			background-repeat: no-repeat;
			height: 80vh;
			border-radius: 50px;

			text-decoration: none;
			color: #000000;
			text-shadow: 0px 2px 4px #000;
			cursor: pointer;
			flex: 0.5;
			margin: 10px;
			position: relative;
			transition: flex 0.7s ease-in;

		}

		.panel h3 {
			font-size: 24px;
			position: absolute;
			bottom: 20px;
			left: 20px;
			margin: 0;
			opacity: 0;
			text-decoration: none;
			color: black;
		}

		.panel.active {
			background-size: 100% auto;
			flex: 5;
		}

		.panel.active h3 {
			opacity: 1;
			transition: opacity 1s ease-in 1s;
		}

		@media (max-width: 480px) {
			.container {
				width: 100vw;
			}

			.panel:nth-of-type(4),
			.panel:nth-of-type(5) {
				display: none;
			}
		}

		.bannerdescriptionShow {
			text-decoration: none;
			color: rgb(0, 0, 0);
		}

		.starss {
			color: orange;
			/* Set the color of the stars */
			font-size: 10px;
		}


		.addToCartBtn {
			background-color: #ffffff;
			color: #000000;
			border: none;
			padding: 10px 15px;
			cursor: pointer;
			display: inline-block;
			position: relative;
			transition: background-color 0.3s ease;
		}

		.addToCartBtn::after {
			margin-left: 8px;
		}

		.addToCartBtn:hover {
			background-color: #ababab;
		}

		.addToCartBtn:focus {
			outline: none;
			/* Remove focus border */
		}
	</style>

